}

.h6:hover::after {
  -webkit-box-shadow: 0 0 0 1px rgba(0, 0, 0, 0.15), 0 4px 6px rgba(0, 0, 0, 0.2);
          box-shadow: 0 0 0 1px rgba(0, 0, 0, 0.15), 0 4px 6px rgba(0, 0, 0, 0.2); /* @include artdeco(Elevation 4dp); */
}

/* Inverse Styles */
.h8 { color: white; }

.h9::before {
  background-color: rgba(255, 255, 255, .42);
}

.h9.h7::before {
  /* The transparent shadow is so that the value can animate on state change */
  -webkit-box-shadow: inset 0 0 0 26px transparent, 0 0 0 1px #98d8f4;
          box-shadow: inset 0 0 0 26px transparent, 0 0 0 1px #98d8f4; /* get-color(blue2) */
}

/* 24dp Styles */
.ha {
  min-height: 24px;
  padding: 8px 48px 8px 0;
}

.ha::before {
  width: 40px;
  height: 24px;
  top: calc(50% - 13px);
  border-radius: 14px;
}

.ha::after {
  width: 20px;
  height: 20px;
  top: calc(50% - 10px);
  right: 19px;
}


/* Toggled Styles */
.hb::before {
  -webkit-box-shadow: inset 0 0 0 26px #0073b1, 0 0 0 1px transparent;
          box-shadow: inset 0 0 0 26px #0073b1, 0 0 0 1px transparent; /* get-color(blue7) */
}

.hb.h7::before {
  -webkit-box-shadow: inset 0 0 0 26px #0073b1, 0 0 0 1px #0073b1;
          box-shadow: inset 0 0 0 26px #0073b1, 0 0 0 1px #0073b1; /* get-color(blue7) */
}

.hb.h6::after {
  right: 3px;
}

.hb.h9.h6::before {
  -webkit-box-shadow: inset 0 0 0 26px #98d8f4, 0 0 0 1px transparent;
          box-shadow: inset 0 0 0 26px #98d8f4, 0 0 0 1px transparent;
}

.hb.h9.h7::before {
  -webkit-box-shadow: inset 0 0 0 26px #98d8f4, 0 0 0 1px #98d8f4;
          box-shadow: inset 0 0 0 26px #98d8f4, 0 0 0 1px #98d8f4; /* get-color(blue2) */
}

/* Disabled Styles */
.hc { opacity: .35; }

.hd.h6 {
  cursor: not-allowed;
}

.hd.h6::before {
  background-color: rgba(0, 0, 0, .45);
}

